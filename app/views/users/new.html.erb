<div class="login">
  <div class="container">
    <div class="form">
    <h1>Sign up</h1>
  <div class="create">
    <div class="container signup">
      <%= form_for(@user) do |f| %>  
    <div class="well"> 
      <%= f.label :first_name %>:
      <%= f.text_field :first_name %><br />
      <%= f.label :last_name %>:
      <%= f.text_field :last_name %><br />
      <%= f.label :email %>:
      <%= f.text_field :email %><br />
      <%= f.label :password %>:
      <%= f.text_field :password %><br />
    </div> 
    <div class="actions"> 
      <%= f.submit "Create" %> 
    </div> 
  <% end %> 
    </div>
  </div>
    </div>
  </div>
</div>